prefix=/workspace/.mamba/envs/pdf-tools
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libarchive
Description: library that can create and read several streaming archive formats
Version: 3.8.1
Cflags: -I${includedir}
Cflags.private: -DLIBARCHIVE_STATIC
Libs: -L${libdir} -larchive
Libs.private: -lcrypto -llzo2 -lzstd -llz4 -lbz2  -lz -L/workspace/.mamba/envs/pdf-tools/lib -llzma -L/workspace/.mamba/envs/pdf-tools/lib -lxml2
Requires.private: libcrypto
